[{"pk": 1, "model": "flatpages.flatpage", "fields": {"registration_required": false, "title": "Readsr Tour", "url": "/tour/", "template_name": "", "sites": [2], "content": "<div id=\"container\" class=\"tour\" > \r\n\r\n<div class=\"tour\" id=\"head\">\r\n<h1 class=\"tour\">Readsr makes finding or promoting a reading series easy.</h1>\r\n</div>\r\n\r\n<div class=\"tour\" id=\"content\">\r\n<p class=\"tour\">Everyone who visits the site sees a list of reading series and a calendar of upcoming readings.\r\n</p>\r\n<p class=\"tour\"><img src=\"/media/tour1.png\" height=\"407\" width=\"600\"></p>\r\n</div>\r\n<div class=\"tour\" id=\"next_parent\">\r\n<div class=\"tour\" id=\"next\">\r\n<a href=\"/tour/2\">Learn all about a series &#187;</a>\r\n</div>\r\n</div>\r\n\r\n</div>", "enable_comments": false}}, {"pk": 2, "model": "flatpages.flatpage", "fields": {"registration_required": false, "title": "Complete series details", "url": "/tour/2/", "template_name": "", "sites": [2], "content": "<div id=\"container\" class=\"tour\" > \r\n\r\n<div class=\"tour\" id=\"head\">\r\n<h1 class=\"tour\">Learn all about a particular series.</h1>\r\n</div>\r\n\r\n<div class=\"tour\" id=\"content\">\r\n<p class=\"tour\">Findn out everything you need to know about a series: when and where, what kinds of writing, and what other people have to say about the series.\r\n</p>\r\n<p class=\"tour\"><img src=\"/media/tour2.png\" height=\"436\" width=\"600\"></p>\r\n</div>\r\n<div class=\"tour\" id=\"next_parent\">\r\n<div class=\"tour\" id=\"next\">\r\n<a href=\"/tour/3\">Benefits of registration &#187;</a>\r\n</div>\r\n</div>\r\n\r\n</div>", "enable_comments": false}}, {"pk": 3, "model": "flatpages.flatpage", "fields": {"registration_required": false, "title": "Start contributing", "url": "/tour/3/", "template_name": "", "sites": [2], "content": "<div id=\"container\" class=\"tour\" > \r\n\r\n<div class=\"tour\" id=\"head\">\r\n<h1 class=\"tour\">You can contribute!</h1>\r\n</div>\r\n\r\n<div class=\"tour\" id=\"content\">\r\n<p class=\"tour\">Once you register, you can contribute to Readsr. For example, add your favorite series so people can find it.\r\n</p>\r\n<p class=\"tour\"><img src=\"/media/tour3.png\" height=\"420\" width=\"600\"></p>\r\n</div>\r\n<div class=\"tour\" id=\"next_parent\">\r\n<div class=\"tour\" id=\"next\">\r\n<a href=\"/tour/4\">Share information &#187;</a>\r\n</div>\r\n</div>\r\n\r\n</div>", "enable_comments": false}}, {"pk": 4, "model": "flatpages.flatpage", "fields": {"registration_required": false, "title": "Tell others what you think", "url": "/tour/4/", "template_name": "", "sites": [2], "content": "<div id=\"container\" class=\"tour\" > \r\n\r\n<div class=\"tour\" id=\"head\">\r\n<h1 class=\"tour\">Tell others what you think.</h1>\r\n</div>\r\n\r\n<div class=\"tour\" id=\"content\">\r\n<p class=\"tour\">Add tags to existing series...\r\n</p>\r\n<p class=\"tour\"><img src=\"/media/tour4.png\" height=\"444\" width=\"600\"></p>\r\n</div>\r\n<div class=\"tour\" id=\"next_parent\">\r\n<div class=\"tour\" id=\"next\">\r\n<a href=\"/tour/5\">Update readings &#187;</a>\r\n</div>\r\n</div>\r\n\r\n</div>", "enable_comments": false}}, {"pk": 5, "model": "flatpages.flatpage", "fields": {"registration_required": false, "title": "Share information", "url": "/tour/5/", "template_name": "", "sites": [2], "content": "<div id=\"container\" class=\"tour\" > \r\n\r\n<div class=\"tour\" id=\"head\">\r\n<h1 class=\"tour\">Share information.</h1>\r\n</div>\r\n\r\n<div class=\"tour\" id=\"content\">\r\n<p class=\"tour\">...and add details about upcoming readings, such as the names of that month's readers.\r\n</p>\r\n<p class=\"tour\"><img src=\"/media/tour5.png\" height=\"420\" width=\"600\"></p>\r\n</div>\r\n<div class=\"tour\" id=\"next_parent\">\r\n<div class=\"tour\" id=\"next\">\r\n<a href=\"/tour/6\">Stay up to date &#187;</a>\r\n</div>\r\n</div>\r\n\r\n</div>", "enable_comments": false}}, {"pk": 6, "model": "flatpages.flatpage", "fields": {"registration_required": false, "title": "Stay up to date with Twitter", "url": "/tour/6/", "template_name": "", "sites": [2], "content": "<div id=\"container\" class=\"tour\" > \r\n\r\n<div class=\"tour\" id=\"head\">\r\n<h1 class=\"tour\">Stay up to date.</h1>\r\n</div>\r\n\r\n<div class=\"tour\" id=\"content\">\r\n<p class=\"tour\">Readsr tweets updates to series such as changes in venue or time. Follow @Readsr and you'll always know when and where your favorites series are happening.\r\n</p>\r\n<p class=\"tour\"><img src=\"/media/tour6.png\" height=\"440\" width=\"600\"></p>\r\n</div>\r\n<div class=\"tour\" id=\"next_parent\">\r\n<div class=\"tour\" id=\"next\">\r\n<a href=\"/tour/7\">More to come &#187;</a>\r\n</div>\r\n</div>\r\n\r\n</div>", "enable_comments": false}}, {"pk": 7, "model": "flatpages.flatpage", "fields": {"registration_required": false, "title": "More on the way", "url": "/tour/7/", "template_name": "", "sites": [2], "content": "<div id=\"container\" class=\"tour\" > \r\n\r\n<div class=\"tour\" id=\"head\">\r\n<h1 class=\"tour\">More on the way.</h1>\r\n</div>\r\n\r\n<div class=\"tour\" id=\"content\">\r\n<p class=\"tour\">Readsr is growing and adding services all the time. Look for features such as series reviews and rich user profiles integrated with social media sites such as Facebook and Twitter soon.\r\n</p>\r\n<p class=\"tour\"><img src=\"/media/tour7.png\" height=\"399\" width=\"600\"></p>\r\n</div>\r\n<div class=\"tour\" id=\"next_parent\">\r\n<div class=\"tour\" id=\"next\">\r\n<a href=\"http://www.readsrs.com/\">Get started using Readsr &#187;</a>\r\n</div>\r\n</div>\r\n\r\n</div>", "enable_comments": false}}]